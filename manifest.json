{
    "manifest_version": 3,
    "name": "AI Phishing Detector Pro",
    "version": "1.0",
    "permissions": [
        "declarativeNetRequest",
        "webNavigation",
        "tabs",
        "storage",
        "notifications",
        "activeTab"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    }
}
